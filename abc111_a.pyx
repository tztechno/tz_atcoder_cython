def transform(input_string: str) -> str:
    cdef dict maps = {'1': '9', '9': '1'}
    cdef int i, n = len(input_string)
    cdef list S = list(input_string)
    cdef list S2 = []
    for i in range(n):
        S2.append(maps.get(S[i], S[i]))
    return ''.join(S2)
S = input()
print(transform(S))
